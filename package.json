{
  "name": "@rubixvi/email-unsend",
  "version": "1.10.4",
  "description": "Payload Unsend Email Adapter",
  "homepage": "https://rubixstudios.com.au",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubixvi/email-unsend.git",
    "directory": "packages/email-unsend"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "email",
    "unsend",
    "adapter",
    "plugin",
    "rubixstudios"
  ],
  "license": "MIT",
  "author": "Rubix Studios <hello@rubixstudios.com.au> (https://rubixstudios.com.au)",
  "maintainers": [
    {
      "name": "Rubix Studios",
      "email": "hello@rubixstudios.com.au",
      "url": "https://rubixstudios.com.au"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc-build --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf -g {dist,*.tsbuildinfo}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm clean && pnpm turbo build",
    "test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "30.0.0-beta.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.31",
    "@swc/jest": "^0.2.38",
    "@types/jest": "29.5.12",
    "@types/node": "^22.15.30",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "turbo": "^2.5.4",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "payload": "^3.28.1"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "access": "public"
  },
  "dependencies": {
    "@payloadcms/eslint-config": "^3.28.0"
  },
  "packageManager": "pnpm@10.11.0"
}
